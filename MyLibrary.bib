@article{takahashiGoalorientedInferenceEnvironment2023,
  title = {Goal-Oriented Inference of Environment from Redundant Observations},
  author = {Takahashi, Kazuki and Fukai, Tomoki and Sakai, Yutaka and Takekawa, Takashi},
  date = {2023},
