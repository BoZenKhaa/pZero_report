@online{heWhatModelDoes2024,
  title = {What Model Does {{MuZero}} Learn?},
  author = {He, Jinke and Moerland, Thomas M. and family=Vries, given=Joery A., prefix=de, useprefix=false and Oliehoek, Frans A.},
  date = {2024-10-12},
  eprint = {2306.00840},
