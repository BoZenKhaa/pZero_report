@unpublished{meichanetzoglouTestingCommonSense2015,
  title = {Testing for Common Sense (Violation) in Airline Pricing or How Complexity Asymmetry Defeated You and the Web},
  author = {Meichanetzoglou, Symeon and Ioannidis, Sotiris and Laoutaris, Nikolaos},
  date = {2015-09-17},
