@article{kumarChaseMeHeuristicScheme2022,
  title = {{{ChaseMe}}: {{A Heuristic Scheme}} for {{Electric Vehicles Mobility Management}} on {{Charging Stations}} in a {{Smart City Scenario}}},
  shorttitle = {{{ChaseMe}}},
  author = {Kumar, Neetesh and Chaudhry, Rashmi and Kaiwartya, Omprakash and Kumar, Neeraj},
