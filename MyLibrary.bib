@article{bayramElectricPowerAllocation2013,
  title = {Electric {{Power Allocation}} in a {{Network}} of {{Fast Charging Stations}}},
  author = {Bayram, I. S. and Michailidis, G. and Devetsikiotis, M. and Granelli, F.},
